<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1"/>
    </head>
    <body>
        <referenceBlock name="head.asset.preload">
            <arguments>
                <argument name="assets" xsi:type="array">
                    <item name="uikit" xsi:type="array">
                        <item name="path" xsi:type="string">js/uikit/uikit.min.js</item>
                        <item name="type" xsi:type="array">
                            <item name="name" xsi:type="string">type</item>
                            <item name="value" xsi:type="string">script</item>
                        </item>
                        <item name="crossorigin" xsi:type="array">
                            <item name="name" xsi:type="string">crossorigin</item>
                            <item name="value" xsi:type="string">false</item>
                        </item>
                    </item>
                    <item name="uikit-icons" xsi:type="array">
                        <item name="path" xsi:type="string">js/uikit/uikit-icons.min.js</item>
                        <item name="type" xsi:type="array">
                            <item name="name" xsi:type="string">type</item>
                            <item name="value" xsi:type="string">script</item>
                        </item>
                        <item name="crossorigin" xsi:type="array">
                            <item name="name" xsi:type="string">crossorigin</item>
                            <item name="value" xsi:type="string">false</item>
                        </item>
                    </item>
                    <item name="montserrat-light" xsi:type="array">
                        <item name="path" xsi:type="string">fonts/Montserrat/Montserrat-Light.woff2</item>
                        <item name="type" xsi:type="array">
                            <item name="name" xsi:type="string">type</item>
                            <item name="value" xsi:type="string">font/woff2</item>
                        </item>
                        <item name="crossorigin" xsi:type="array">
                            <item name="name" xsi:type="string">crossorigin</item>
                            <item name="value" xsi:type="string">false</item>
                        </item>
                    </item>
                    <item name="montserrat-regular" xsi:type="array">
                        <item name="path" xsi:type="string">fonts/Montserrat/Montserrat-Regular.woff2</item>
                        <item name="type" xsi:type="array">
                            <item name="name" xsi:type="string">type</item>
                            <item name="value" xsi:type="string">font/woff2</item>
                        </item>
                        <item name="crossorigin" xsi:type="array">
                            <item name="name" xsi:type="string">crossorigin</item>
                            <item name="value" xsi:type="string">false</item>
                        </item>
                    </item>
                    <item name="montserrat-bold" xsi:type="array">
                        <item name="path" xsi:type="string">fonts/Montserrat/Montserrat-Bold.woff2</item>
                        <item name="type" xsi:type="array">
                            <item name="name" xsi:type="string">type</item>
                            <item name="value" xsi:type="string">font/woff2</item>
                        </item>
                        <item name="crossorigin" xsi:type="array">
                            <item name="name" xsi:type="string">crossorigin</item>
                            <item name="value" xsi:type="string">false</item>
                        </item>
                    </item>
                    <item name="black_icons" xsi:type="array">
                        <item name="path" xsi:type="string">fonts/Blank-Theme-Icons/Blank-Theme-Icons.woff2</item>
                        <item name="type" xsi:type="array">
                            <item name="name" xsi:type="string">type</item>
                            <item name="value" xsi:type="string">font/woff2</item>
                        </item>
                        <item name="crossorigin" xsi:type="array">
                            <item name="name" xsi:type="string">crossorigin</item>
                            <item name="value" xsi:type="string">false</item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="head.additional">
            <block name="head.uikit.init" template="MageOS_Theme::uikit-init.phtml"/>
        </referenceBlock>
    </body>
</page>
